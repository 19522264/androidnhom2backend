generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  uid String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  password String
}
model userprofile{ 
  docid String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique 
  displayName String
  photoURL String
}
